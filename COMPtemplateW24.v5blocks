)b2I#XzEG,I5\"><field name=\"TARGET\">Brain</field><value name=\"ROW\"><shadow type=\"math_whole_number\" id=\"Wv](S@M+|FRvVy,J`!B,\"><field name=\"NUM\">6</field></shadow></value><value name=\"COLUMN\"><shadow type=\"math_whole_number\" id=\"xq?36Qe,UcQ2?{yW|dUG\"><field name=\"NUM\">1</field></shadow></value><next><block type=\"v5_looks_print\" id=\"FqF!LUt|W.}Wb=QY.nT=\"><field name=\"TARGET\">Brain</field><field name=\"andsetcursortonextrow_mutator\">false</field><value name=\"DATA\"><shadow type=\"text\" id=\"!KR(5*Ic%[c:1=Ol9%+E\"><field name=\"TEXT\">Rotate degrees:</field></shadow></value><next><block type=\"v5_looks_print\" id=\"1)Z=c)Cv4-Iwn%+-2r1/\"><field name=\"TARGET\">Brain</field><field name=\"andsetcursortonextrow_mutator\">false</field><value name=\"DATA\"><shadow xmlns=\"http://www.w3.org/1999/xhtml\" type=\"text\" id=\".9|[[Nux:l.1S/,X~K@S\"><field name=\"TEXT\">VEXcode</field></shadow><block type=\"v5_sensing_position_of_motor\" id=\"keCWIWlqlhdP^5L$Xc0G\"><field name=\"MOTOR\">rotateM</field><field name=\"UNITS\">deg</field></block></value></block></next></block></next></block></next></block></next></block></next></block></next></block></next></block></xml>","robotConfig":[{"port":[],"name":"Controller1","customName":false,"deviceType":"Controller","deviceClass":"controller","setting":{"left":"","leftDir":"false","right":"","rightDir":"false","upDown":"","upDownDir":"false","xB":"","xBDir":"false","drive":"none","id":"primary"},"triportSourcePort":22},{"port":[11,20],"name":"ArmLiftMG","customName":true,"deviceType":"MotorGroup","deviceClass":"motor_group","setting":{"fwd":"forward","rev":"reverse","gear":"ratio18_1","motor_a_reversed":"true","motor_b_reversed":"false"},"triportSourcePort":22},{"port":[10],"name":"LeftM","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1","id":"partner"},"triportSourcePort":22},{"port":[1],"name":"RightM","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"true","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[18],"name":"grabberM","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1","id":"partner"},"triportSourcePort":22},{"port":[19],"name":"rotateM","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1","id":"partner"},"triportSourcePort":22}],"slot":6,"platform":"V5","sdkVersion":"20240802.15.00.00","appVersion":"4.0.5","minVersion":"3.1.0","fileFormat":"2.0.0","targetBrainGen":"First","v5Sounds":[{"name":"game over","url":"static/sounds/mixkit-arcade-retro-game-over-213.wav"}],"v5SoundsEnabled":false,"downloadLanguage":"python","cppStatus":"true","cpp":"// Make sure all required headers are included.\n#include <stdio.h>\n#include <stdlib.h>\n#include <stdbool.h>\n#include <math.h>\n#include <string.h>\n\n\n#include \"vex.h\"\n\nusing namespace vex;\n\n// Brain should be defined by default\nbrain Brain;\n\n\n// START V5 MACROS\n#define waitUntil(condition)                                                   \\\n  do {                                                                         \\\n    wait(5, msec);                                                             \\\n  } while (!(condition))\n\n#define repeat(iterations)                                                     \\\n  for (int iterator = 0; iterator < iterations; iterator++)\n// END V5 MACROS\n\n\n// Robot configuration code.\ncontroller Controller1 = controller(primary);\nmotor ArmLiftMGMotorA = motor(PORT11, ratio18_1, true);\nmotor ArmLiftMGMotorB = motor(PORT20, ratio18_1, false);\nmotor_group ArmLiftMG = motor_group(ArmLiftMGMotorA, ArmLiftMGMotorB);\n\nmotor LeftM = motor(PORT10, ratio18_1, false);\n\nmotor RightM = motor(PORT1, ratio18_1, true);\n\nmotor grabberM = motor(PORT18, ratio18_1, false);\n\nmotor rotateM = motor(PORT19, ratio18_1, false);\n\n\n\n// generating and setting random seed\nvoid initializeRandomSeed(){\n  int systemTime = Brain.Timer.systemHighResolution();\n  double batteryCurrent = Brain.Battery.current();\n  double batteryVoltage = Brain.Battery.voltage(voltageUnits::mV);\n\n  // Combine these values into a single integer\n  int seed = int(batteryVoltage + batteryCurrent * 100) + systemTime;\n\n  // Set the seed\n  srand(seed);\n}\n\n\n\nvoid vexcodeInit() {\n\n  //Initializing random seed.\n  initializeRandomSeed(); \n}\n\n\n\n\n// Helper to make playing sounds from the V5 in VEXcode easier and\n// keeps the code cleaner by making it clear what is happening.\nvoid playVexcodeSound(const char *soundName) {\n  printf(\"VEXPlaySound:%s\\n\", soundName);\n  wait(5, msec);\n}\n\n\n\n// Generated code.\n\n\n\n// define variable for remote controller enable/disable\nbool RemoteControlCodeEnabled = true;\n\n\n\n// Include the V5 Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\ncompetition Competition;\n\nint Brain_precision = 0, Console_precision = 0, Controller1_precision = 0;\n\nfloat myVariable, speedForward, speedPlusState, speedFactor, speedScale;\n\nevent message1 = event();\nevent AUTONOMOUS = event();\nevent DRIVER = event();\n\n// Used to find the format string for printing numbers with the\n// desired number of decimal places\nconst char* printToBrain_numberFormat() {\n  // look at the current precision setting to find the format string\n  switch(Brain_precision){\n    case 0:  return \"%.0f\"; // 0 decimal places (1)\n    case 1:  return \"%.1f\"; // 1 decimal place  (0.1)\n    case 2:  return \"%.2f\"; // 2 decimal places (0.01)\n    case 3:  return \"%.3f\"; // 3 decimal places (0.001)\n    default: return \"%f\"; // use the print system default for everthing else\n  }\n}\n\n// \"when started\" hat block\nint whenStarted1() {\n  Brain.Screen.setCursor(1, 1);\n  Brain.Screen.print(\"Program Started.....\");\n  Brain.Screen.newLine();\n  ArmLiftMG.setPosition(0.0, degrees);\n  ArmLiftMG.setStopping(hold);\n  ArmLiftMG.setVelocity(50.0, rpm);\n  grabberM.setPosition(0.0, degrees);\n  grabberM.setStopping(hold);\n  grabberM.setVelocity(50.0, rpm);\n  rotateM.spinToPosition(0.0, degrees, true);\n  rotateM.setStopping(hold);\n  rotateM.setVelocity(50.0, rpm);\n  Brain_precision = 1;\n  // When testing broadbast blocks should be enabled to\n  // to trigger autnomous followd by driver - it is also possible\n  // to just enable driver by disabling broadcast autonomous block\n  DRIVER.broadcast();\n  while (true) {\n    Brain.Screen.setCursor(4, 1);\n    Brain.Screen.print(\"ArmPosition:\");\n    Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(ArmLiftMG.position(degrees)));\n    Brain.Screen.setCursor(5, 1);\n    Brain.Screen.print(\"Grabber Position:\");\n    Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(grabberM.position(degrees)));\n    Brain.Screen.setCursor(6, 1);\n    Brain.Screen.print(\"Rotate Position:\");\n    Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(rotateM.position(degrees)));\n    wait(0.1, seconds);\n  wait(5, msec);\n  }\n  return 0;\n}\n\n// \"when I receive AUTONOMOUS\" hat block\nvoid onevent_AUTONOMOUS_0() {\n  RemoteControlCodeEnabled = false;\n  Brain.Screen.setCursor(3, 1);\n  Brain.Screen.print(\"Starting Autonomous Program\");\n  Brain.Screen.newLine();\n  ArmLiftMG.stop();\n  ArmLiftMG.setPosition(0.0, degrees);\n  ArmLiftMG.setMaxTorque(80.0, percent);\n  ArmLiftMG.setVelocity(50.0, rpm);\n  // Drive 4000mm forward at 125RPM (green cart.)\n  // Dwheel=100mm -- distance=4000mm -- degrees= 360*(4000/3.14*100)\n  LeftM.spinFor(forward, 4585.0, degrees, false);\n  RightM.spinFor(forward, 4585.0, degrees, true);\n  LeftM.stop();\n  RightM.stop();\n  // Make 90 degree right turn\n  // 90deg turn = 1/4 of turn circle, 400mm trackwidth = 360deg\n  // axelDeg = ((deg/360 * trackCirm)/wheelCirc)*360\n  // axelDeg = 360Deg -- 180Deg fw and 180Deg bckwd for pivot\n  LeftM.spinFor(forward, 180.0, degrees, true);\n  RightM.spinFor(reverse, 180.0, degrees, true);\n  LeftM.stop();\n  RightM.stop();\n  // Move ARM 45 deg. UP -- this means the motor must turn 7 * 45 = 315 degrees\n  // Remember that the gear ratio is 1:7 (12:84 gear ratio) and does the actual movement\n  // of the arm must be calculated  as the motor axel turn (12t gear)\n  ArmLiftMG.spinToPosition(315.0, degrees, true);\n  ArmLiftMG.stop();\n  Brain.Screen.setCursor(7, 1);\n  Brain.Screen.print(\"Ended Autonomous Run...\");\n  Brain.Screen.newLine();\n}\n\n// \"when autonomous\" hat block\nint onauton_autonomous_0() {\n  return 0;\n}\n\n// \"when I receive DRIVER\" hat block\nvoid onevent_DRIVER_0() {\n  RemoteControlCodeEnabled = true;\n  Brain.Screen.setCursor(2, 1);\n  Brain.Screen.print(\"Driver Control Started....\");\n  Brain.Screen.newLine();\n  Brain.Screen.setCursor(4, 1);\n  Brain.Screen.print(\"arm degrees:\");\n  Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(ArmLiftMG.position(degrees)));\n  // start speed at 100%\n  speedScale = 1.0;\n  speedFactor = 1.0;\n  // Tank Control Drive\n  // Replace above Forever wit this stack - if you want to use Arcade Drive - left joystick speed F/B\n  // right Joystick turn left or right\n  while (true) {\n    LeftM.setVelocity(((Controller1.Axis3.position() + Controller1.Axis1.position()) * speedFactor), percent);\n    RightM.setVelocity(((Controller1.Axis3.position() - Controller1.Axis1.position()) * speedFactor), percent);\n    LeftM.spin(forward);\n    RightM.spin(forward);\n  wait(5, msec);\n  }\n}\n\n// \"when driver control\" hat block\nint ondriver_drivercontrol_0() {\n  return 0;\n}\n\n// \"when Controller1 ButtonR1 pressed\" hat block\nvoid onevent_Controller1ButtonR1_pressed_0() {\n  // Arm move UPWARDS control\n  ArmLiftMG.spin(forward);\n  waitUntil((!Controller1.ButtonR1.pressing()));\n  ArmLiftMG.stop();\n  Brain.Screen.setCursor(4, 1);\n  Brain.Screen.print(\"arm degrees:\");\n  Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(ArmLiftMG.position(degrees)));\n}\n\n// \"when Controller1 ButtonLeft pressed\" hat block\nvoid onevent_Controller1ButtonLeft_pressed_0() {\n  // Grabber Open movement\n  grabberM.spin(forward);\n  waitUntil((!Controller1.ButtonLeft.pressing()));\n  grabberM.stop();\n  Brain.Screen.setCursor(5, 1);\n  Brain.Screen.print(\"Grabber degrees:\");\n  Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(grabberM.position(degrees)));\n}\n\n// \"when Controller1 ButtonR2 pressed\" hat block\nvoid onevent_Controller1ButtonR2_pressed_0() {\n  // Arm move DOWNWARDS control\n  ArmLiftMG.spin(reverse);\n  waitUntil((!Controller1.ButtonR2.pressing()));\n  ArmLiftMG.stop();\n  Brain.Screen.setCursor(4, 1);\n  Brain.Screen.print(\"arm degrees:\");\n  Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(ArmLiftMG.position(degrees)));\n}\n\n// \"when Controller1 ButtonRight pressed\" hat block\nvoid onevent_Controller1ButtonRight_pressed_0() {\n  // Grabber close movement\n  grabberM.spin(reverse);\n  waitUntil((!Controller1.ButtonRight.pressing()));\n  grabberM.stop();\n  Brain.Screen.setCursor(5, 1);\n  Brain.Screen.print(\"Grabber degrees:\");\n  Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(grabberM.position(degrees)));\n}\n\n// \"when Controller1 ButtonY pressed\" hat block\nvoid onevent_Controller1ButtonY_pressed_0() {\n  // Grabber rotate movement CW\n  rotateM.spin(forward);\n  waitUntil((!Controller1.ButtonY.pressing()));\n  rotateM.stop();\n  Brain.Screen.setCursor(6, 1);\n  Brain.Screen.print(\"Rotate degrees:\");\n  Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(rotateM.position(degrees)));\n}\n\n// \"when Controller1 ButtonL1 pressed\" hat block\nvoid onevent_Controller1ButtonL1_pressed_0() {\n  // Updating drivebase speed scaling\n  // Update Scaling 1 = 100% 2 = 75% 3 = 50% 4 = 25%\n  if (speedScale == 1.0) {\n    speedScale = 4.0;\n  }\n  else {\n    speedScale = speedScale + -1.0;\n  }\n  waitUntil((!Controller1.ButtonL1.pressing()));\n  if (speedScale == 1.0) {\n    speedFactor = 1.0;\n  } else if (speedScale == 2.0) {\n    speedFactor = 0.75;\n  } else if (speedScale == 3.0) {\n    speedFactor = 0.5;\n  } else {\n    speedFactor = 0.25;\n  }\n}\n\n// \"when Controller1 ButtonL2 pressed\" hat block\nvoid onevent_Controller1ButtonL2_pressed_0() {\n  // Updating drivebase speed scaling\n  // Update Scaling 1 = 100% 2 = 75% 3 = 50% 4 = 25%\n  if (speedScale == 4.0) {\n    speedScale = 1.0;\n  }\n  else {\n    speedScale = speedScale + 1.0;\n  }\n  waitUntil((!Controller1.ButtonL2.pressing()));\n  if (speedScale == 1.0) {\n    speedFactor = 1.0;\n  } else if (speedScale == 2.0) {\n    speedFactor = 0.75;\n  } else if (speedScale == 3.0) {\n    speedFactor = 0.5;\n  } else {\n    speedFactor = 0.25;\n  }\n}\n\n// \"when Controller1 ButtonA pressed\" hat block\nvoid onevent_Controller1ButtonA_pressed_0() {\n  // Grabber rotate movement CCW\n  rotateM.spin(reverse);\n  waitUntil((!Controller1.ButtonA.pressing()));\n  rotateM.stop();\n  Brain.Screen.setCursor(6, 1);\n  Brain.Screen.print(\"Rotate degrees:\");\n  Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(rotateM.position(degrees)));\n}\n\nvoid VEXcode_driver_task() {\n  // Start the driver control tasks....\n  vex::task drive0(ondriver_drivercontrol_0);\n  while(Competition.isDriverControl() && Competition.isEnabled()) {this_thread::sleep_for(10);}\n  drive0.stop();\n  return;\n}\n\nvoid VEXcode_auton_task() {\n  // Start the auton control tasks....\n  vex::task auto0(onauton_autonomous_0);\n  while(Competition.isAutonomous() && Competition.isEnabled()) {this_thread::sleep_for(10);}\n  auto0.stop();\n  return;\n}\n\n\n\nint main() {\n  vex::competition::bStopTasksBetweenModes = false;\n  Competition.autonomous(VEXcode_auton_task);\n  Competition.drivercontrol(VEXcode_driver_task);\n\n  // Initializing Robot Configuration. DO NOT REMOVE!\n  vexcodeInit();\n\n  // register event handlers\n  AUTONOMOUS(onevent_AUTONOMOUS_0);\n  DRIVER(onevent_DRIVER_0);\n  Controller1.ButtonR1.pressed(onevent_Controller1ButtonR1_pressed_0);\n  Controller1.ButtonLeft.pressed(onevent_Controller1ButtonLeft_pressed_0);\n  Controller1.ButtonR2.pressed(onevent_Controller1ButtonR2_pressed_0);\n  Controller1.ButtonRight.pressed(onevent_Controller1ButtonRight_pressed_0);\n  Controller1.ButtonY.pressed(onevent_Controller1ButtonY_pressed_0);\n  Controller1.ButtonL1.pressed(onevent_Controller1ButtonL1_pressed_0);\n  Controller1.ButtonL2.pressed(onevent_Controller1ButtonL2_pressed_0);\n  Controller1.ButtonA.pressed(onevent_Controller1ButtonA_pressed_0);\n\n  wait(15, msec);\n  // post event registration\n\n  // set default print color to black\n  printf(\"\\033[30m\");\n\n  // wait for rotation sensor to fully initialize\n  wait(30, msec);\n\n  whenStarted1();\n}","target":"Physical"}